{
  "name": "softball-scorer",
  "version": "1.0.0",
  "description": "This program tracks scores softball teams.",
  "main": "index.js",
  "scripts": {
    "run": "node src-srv",
    "build": "node build.js build",
    "watch": "webpack --watch",
    "build-css": "node build.js build-css",
    "clean": "node build.js clean",
    "start": "node src-srv",
    "test": "jest --runInBand --detectOpenHandles --bail --verbose=false --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thbrown/softball-scorer.git"
  },
  "author": "Browns and Co.",
  "license": "",
  "bugs": {
    "url": "https://github.com/thbrown/softball-scorer/issues"
  },
  "babel": {
    "plugins": [
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ]
        }
      ]
    ],
    "presets": [
      "@babel/preset-react",
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": "last 1 chrome versions"
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "no-throw-literal": "off",
      "no-lone-blocks": "off"
    }
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src-srv/test/jest-setup.js"
    ]
  },
  "homepage": "https://github.com/thbrown/softball-scorer#readme",
  "dependencies": {
    "base-x": "^3.0.4",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.18.3",
    "connect-redis": "^3.4.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "file-saver": "^2.0.0-rc.3",
    "folder-hash": "^2.1.1",
    "got": "^8.3.2",
    "helmet": "^3.12.1",
    "ip": "^1.1.5",
    "lz-string": "^1.4.4",
    "mailgun-js": "^0.22.0",
    "marked": "^0.6.2",
    "object-hash": "^1.3.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.4.1",
    "react": "^16.2.0",
    "react-autosuggest": "^9.3.4",
    "react-dom": "^16.2.0",
    "react-dom-factories": "^1.0.2",
    "react-draggable": "^3.0.5",
    "react-select": "^2.4.2",
    "react-sticky-table": "2.0.0",
    "reactify": "^1.1.1",
    "redis": "^2.8.0",
    "serve-favicon": "^2.5.0",
    "stable": "^0.1.8",
    "timsort": "^0.3.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "babelify": "^10.0.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^3.10.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "jest": "^24.7.1",
    "node-fetch": "^2.6.0",
    "prettier": "^1.18.2",
    "terser": "^4.0.0",
    "uglifycss": "0.0.26",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.4"
  },
  "config": {
    "vendor_modules": [
      "react",
      "react-dom"
    ],
    "bundles": {
      "vendor": "build/vendor.js",
      "main": {
        "out": "build/main.js",
        "entry": "src/index.js"
      }
    }
  }
}
