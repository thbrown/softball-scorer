{
  "name": "softball-scorer",
  "version": "1.0.0",
  "description": "This program assists with the tracking of stats for softball teams.",
  "main": "index.js",
  "scripts": {
    "run": "node src-srv",
    "start": "node src-srv",
    "start:dev": "yarn watch",
    "build": "node build.js build",
    "watch": "rm -rf build/*hot-update.json build/*.hot-update.js && concurrently -n srv,webpack \"node src-srv\" \"node_modules/.bin/webpack serve --env development --config webpack.dev.js --progress\"",
    "build-css": "node build.js build-css",
    "update-service-worker": "node build.js update-service-worker",
    "clean": "node build.js clean",
    "test": "jest --runInBand --detectOpenHandles --bail --verbose=false --forceExit",
    "test:ui": "jest --watch ./src/tests/*",
    "analyze": "webpack --config webpack.prod.js --profile --json > build/stats.json && webpack-bundle-analyzer build/stats.json",
    "fix": "npm i --package-lock-only && npm audit fix && rm yarn.lock && yarn import && rm package-lock.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thbrown/softball-scorer.git"
  },
  "author": "Browns and Co.",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/thbrown/softball-scorer/issues"
  },
  "babel": {
    "plugins": [
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ]
        }
      ],
      "@babel/plugin-proposal-optional-chaining",
      "@babel/plugin-proposal-class-properties"
    ],
    "presets": [
      "@babel/preset-react",
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": "last 1 chrome versions"
          }
        }
      ]
    ]
  },
  "jest": {
    "testEnvironment": "enzyme",
    "testEnvironmentOptions": {
      "enzymeAdapter": "react16"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src-srv/test/jest-setup.js",
      "./node_modules/jest-enzyme/lib/index.js"
    ]
  },
  "homepage": "https://github.com/thbrown/softball-scorer#readme",
  "resolutions": {
    "node-fetch": "^2.6.7"
  },
  "dependencies": {
    "acorn": "^8.0.4",
    "base-x": "^3.0.6",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.18.3",
    "buffer": "^6.0.3",
    "connect-redis": "^5.0.0",
    "cookie-parser": "^1.4.4",
    "crypto-browserify": "^3.12.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "file-saver": "^2.0.0-rc.3",
    "folder-hash": "^4.0.0",
    "googleapis": "^66.0.0",
    "got": "^11.8.0",
    "helmet": "^4.2.0",
    "lz-string": "^1.4.4",
    "mailgun.js": "^2.0.6",
    "node-object-hash": "^2.0.0",
    "object-hash": "^2.0.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^8.5.1",
    "process": "^0.11.10",
    "react": "17.0.1",
    "react-autosuggest": "^10.0.4",
    "react-dom": "17.0.1",
    "react-dom-factories": "^1.0.2",
    "react-draggable": "^4.4.3",
    "react-select": "^3.1.1",
    "react-sticky-table": "2.0.0",
    "reactify": "^1.1.1",
    "recompose": "^0.30.0",
    "redis": "^3.0.2",
    "serve-favicon": "^2.5.0",
    "stable": "^0.1.8",
    "stream-browserify": "^3.0.0",
    "uuid": "^8.3.1",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.3.2",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^4.0.0",
    "before-build-webpack": "^0.2.9",
    "concurrently": "^5.0.2",
    "cross-env": "^7.0.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "jest-enzyme": "^7.1.1",
    "node-fetch": "^2.6.7",
    "prettier": "^2.2.1",
    "set-cookie-parser": "^2.4.6",
    "terser": "^5.5.1",
    "uglifycss": "0.0.29",
    "webpack": "^5.10.0",
    "webpack-bundle-analyzer": "^4.2.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^4.7.4",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "config": {
    "bundles": {
      "main": {
        "out": "build/main.js",
        "entry": "src/index.js"
      }
    }
  }
}
