{
  "name": "softball-scorer",
  "version": "1.0.0",
  "description": "This program tracks scores softball teams.",
  "main": "index.js",
  "scripts": {
    "run": "node src-srv",
    "start": "node src-srv",
    "build": "node build.js build",
    "watch": "concurrently -n srv,webpack \"node src-srv\" \"webpack-dev-server --client-log-level error  --config webpack.dev.js\"",
    "build-css": "node build.js build-css",
    "clean": "node build.js clean",
    "test-watch": "jest --watch ./src/tests/*",
    "test": "jest --runInBand --detectOpenHandles --bail --verbose=false --forceExit",
    "analyze": "webpack --config webpack.prod.js --profile --json > build/stats.json && webpack-bundle-analyzer build/stats.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thbrown/softball-scorer.git"
  },
  "author": "Browns and Co.",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/thbrown/softball-scorer/issues"
  },
  "babel": {
    "plugins": [
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ]
        }
      ],
      "@babel/plugin-proposal-optional-chaining",
      "@babel/plugin-proposal-class-properties"
    ],
    "presets": [
      "@babel/preset-react",
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": "last 1 chrome versions"
          }
        }
      ]
    ]
  },
  "jest": {
    "testEnvironment": "enzyme",
    "testEnvironmentOptions": {
      "enzymeAdapter": "react16"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src-srv/test/jest-setup.js",
      "./node_modules/jest-enzyme/lib/index.js"
    ]
  },
  "homepage": "https://github.com/thbrown/softball-scorer#readme",
  "dependencies": {
    "acorn": "^6.1.1",
    "base-x": "^3.0.6",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.18.3",
    "connect-redis": "^3.4.0",
    "cookie-parser": "^1.4.4",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "file-saver": "^2.0.0-rc.3",
    "folder-hash": "^2.1.1",
    "googleapis": "^40.0.0",
    "got": "^8.3.2",
    "helmet": "^3.12.1",
    "ip": "^1.1.5",
    "lz-string": "^1.4.4",
    "mailgun-js": "^0.22.0",
    "marked": "^0.7.0",
    "node-object-hash": "^2.0.0",
    "object-hash": "^2.0.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.4.1",
    "react": "16.8.6",
    "react-autosuggest": "^9.3.4",
    "react-dom": "16.8.6",
    "react-dom-factories": "^1.0.2",
    "react-draggable": "^3.0.5",
    "react-jss": "^8.6.1",
    "react-select": "^2.4.2",
    "react-sticky-table": "2.0.0",
    "reactify": "^1.1.1",
    "recompose": "^0.30.0",
    "redis": "^2.8.0",
    "serve-favicon": "^2.5.0",
    "stable": "^0.1.8",
    "uuid": "^3.2.1",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "before-build-webpack": "^0.2.9",
    "concurrently": "^5.0.2",
    "cross-env": "^6.0.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "latest",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^3.10.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "jest": "^24.9.0",
    "jest-enzyme": "^7.1.1",
    "node-fetch": "^2.6.0",
    "prettier": "^1.18.2",
    "terser": "^4.0.0",
    "uglifycss": "0.0.26",
    "webpack": "^4.35.3",
    "webpack-bundle-analyzer": "^3.5.2",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.10.1"
  },
  "config": {
    "bundles": {
      "main": {
        "out": "build/main.js",
        "entry": "src/index.js"
      }
    }
  }
}
