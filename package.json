{
  "name": "softball-scorer",
  "version": "1.0.0",
  "description": "This program assists with the tracking of stats for softball teams.",
  "main": "index.js",
  "scripts": {
    "run": "node src-srv",
    "start": "node src-srv",
    "start:dev": "yarn watch",
    "build": "yarn bundle && node build.js build",
    "bundle": "webpack --progress",
    "bundle-watch": "webpack serve --env WEBPACK_DEV_SERVER=true --progress",
    "watch": "concurrently -n srv,webpack \"yarn start\" \"yarn bundle-watch\"",
    "build-css": "node build.js build-css",
    "build-shared": "(cd shared && yarn build)",
    "update-service-worker": "node build.js update-service-worker",
    "clean": "node build.js clean",
    "test": "jest --runInBand --bail",
    "test-old": "jest --runInBand --detectOpenHandles --bail --verbose=false --forceExit",
    "test:ui": "jest --watch ./src/tests/*",
    "analyze": "webpack --profile --json > build/stats.json && webpack-bundle-analyzer build/stats.json",
    "fix": "npm i --package-lock-only && npm audit fix && rm yarn.lock && yarn import && rm package-lock.json",
    "fmt": "prettier src src-srv --write",
    "fmt:check": "prettier src src-srv --check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thbrown/softball-scorer.git"
  },
  "author": "Browns and Co.",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/thbrown/softball-scorer/issues"
  },
  "jest": {
    "preset": "ts-jest",
    "transform": {
      "^.+\\.(ts|tsx)?$": "ts-jest",
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "testEnvironment": "enzyme",
    "testEnvironmentOptions": {
      "enzymeAdapter": "react16"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src-srv/test/jest-setup.js",
      "./node_modules/jest-enzyme/lib/index.js"
    ]
  },
  "homepage": "https://github.com/thbrown/softball-scorer#readme",
  "resolutions": {
    "node-fetch": "^2.6.7"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.8.1",
    "acorn": "^8.0.4",
    "ajv": "^8.11.0",
    "base-x": "^3.0.6",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.18.3",
    "buffer": "^6.0.3",
    "connect-redis": "^5.0.0",
    "cookie-parser": "^1.4.4",
    "crypto-browserify": "^3.12.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "fast-json-patch": "^3.1.1",
    "file-saver": "^2.0.0-rc.3",
    "gcp-bucket-session-store": "^0.2.0",
    "googleapis": "^66.0.0",
    "got": "^11.8.0",
    "helmet": "^4.2.0",
    "jsonpointer": "^5.0.1",
    "lz-string": "^1.4.4",
    "mailgun.js": "^6.0.1",
    "node-object-hash": "^2.3.10",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "process": "^0.11.10",
    "react": "18.2.0",
    "react-autosuggest": "^10.0.4",
    "react-dom": "18.2.0",
    "react-draggable": "^4.4.3",
    "react-select": "^3.1.1",
    "react-sticky-table": "2.0.0",
    "reactify": "^1.1.1",
    "recompose": "^0.30.0",
    "redis": "^3.0.2",
    "stream-browserify": "^3.0.0",
    "trystero": "^0.17.0",
    "uuid": "^8.3.1",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-syntax-jsx": "^7.18.6",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "@peculiar/webcrypto": "^1.4.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.3.2",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^29.3.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^4.0.0",
    "before-build-webpack": "^0.2.9",
    "concurrently": "^5.0.2",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "esbuild-loader": "^2.20.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "jest": "^26.6.3",
    "jest-enzyme": "^7.1.2",
    "json-loader": "^0.5.7",
    "node-fetch": "^2.6.7",
    "prettier": "^2.2.1",
    "set-cookie-parser": "^2.4.6",
    "terser": "^5.5.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "uglifycss": "0.0.29",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.2.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "config": {
    "bundles": {
      "main": {
        "out": "build/main.js",
        "entry": "src/index.js"
      }
    }
  }
}
