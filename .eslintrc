{
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    }
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-use-before-define": [2, "nofunc"],
    "no-mixed-spaces-and-tabs": 2,
    "semi": [2, "always"],
    "no-alert": 2,
    "no-array-constructor": 2,
    "no-caller": 2,
    "no-catch-shadow": 2,
    "no-eval": 2,
    "no-extra-bind": 0,
    "no-implied-eval": 2,
    "no-iterator": 2,
    "no-label-var": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-multi-spaces": 2,
    "no-native-reassign": 2,
    "no-new": 2,
    "no-new-func": 2,
    "no-new-object": 2,
    "no-new-wrappers": 2,
    "no-octal-escape": 2,
    "no-proto": 2,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-sequences": 2,
    "no-shadow-restricted-names": 2,
    "no-spaced-func": 2,
    "no-trailing-spaces": 2,
    "no-undef-init": 2,
    "no-with": 2,
    "react/jsx-uses-vars": 2,

    "consistent-return": 0,
    "curly": [2, "all"],
    "dot-notation": [2, {
      "allowKeywords": true
    }],
    "eol-last": 2,
    "no-extra-parens": [2, "functions"],
    "eqeqeq": 2,
    "new-parens": 2,
    "semi-spacing": [2, {
      "before": false,
      "after": true
    }],
    "keyword-spacing": 0,
    "space-unary-ops": [2, {
      "words": true,
      "nonwords": false
    }],
    "yoda": [2, "never"],

    "no-sparse-arrays": 0,
    "no-empty": 0,
    "no-loop-func": 0,
    "no-inner-declarations": 0,
    "no-console": 0,
    "no-multi-str": 0,
    "space-infix-ops": 0,
    "quotes": 0,
    "new-cap": 0,
    "no-redeclare": 0,
    "camelcase": 0,
    "no-shadow": 0,
    "comma-spacing": 0,
    "key-spacing": 0,
    "no-underscore-dangle": 0,
    "comma-dangle": 0,
    "no-extend-native": 0,
    "no-unused-expressions": 0,
  }
}
