{
  "name": "client",
  "version": "1.0.0",
  "main": "src/index.html",
  "license": "MIT",
  "scripts": {
    "start": "vite serve src --config ./vite.config.ts",
    "start:dev": "yarn start",
    "build": "yarn typecheck && yarn build:css && yarn clean && vite build src --config ./vite.config.ts",
    "build:css": "tsx scripts/build-css",
    "clean": "rm -rf ../build",
    "typecheck": "tsc",
    "lint:check": "eslint --ext .ts,.tsx,.js,.jsx src",
    "lint:fix": "eslint --ext .ts,.tsx,.js,.jsx --fix src",
    "test": "vitest --coverage",
    "test:prod": "vitest run"
  },
  "devDependencies": {
    "@testing-library/dom": "^9.0.1",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/ui": "^0.29.7",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.8.0",
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "happy-dom": "^8.9.0",
    "lz-string": "^1.4.4",
    "react": "18.2.0",
    "react-autosuggest": "^10.0.4",
    "react-dom": "18.2.0",
    "react-draggable": "^4.4.3",
    "react-select": "^3.1.1",
    "react-sticky-table": "2.0.0",
    "reactify": "^1.1.1",
    "recompose": "^0.30.0",
    "rollup-plugin-copy": "^3.5.0",
    "shared-lib": "1.0.0",
    "uglifycss": "^0.0.29",
    "vite-plugin-node-polyfills": "^0.21.0"
  }
}
